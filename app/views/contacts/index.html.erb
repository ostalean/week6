<h1>Contacts</h1>

<% contacts = Contact.all %>  
<% companies = Company.all%>
<ul>
<% for contact in contacts %>
  <li>
    <%= contact["first_name"] %>
    <%= contact["last_name"]%>
     <% company = companies.find_by({"id" => contact["company_id"]})%>
    (<%= company["name"] %>)
  </li>
  <% end %>
</ul>